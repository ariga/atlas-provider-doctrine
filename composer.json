{
    "name": "ariga/atlas-provider-doctrine",
    "description": "Atlas provider for PHP doctrine ORM",
    "type": "library",
    "require": {
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Provider\\": "src/"
        }
    }
}
